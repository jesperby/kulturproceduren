<h1>Editing questionaire</h1>

<% form_for(@questionaire) do |f| %>
  <%= f.error_messages :id => nil, :class => "form-error-messages" %>

  <p>
    <%= f.label :event_id %><br />
    <%= f.text_field :event_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @questionaire %> |
<%= link_to 'Back', questionaires_path %>
