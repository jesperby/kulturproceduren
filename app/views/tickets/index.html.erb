<h1>Listing tickets</h1>

<table>
  <tr>
    <th>State</th>
    <th>Group</th>
    <th>Event</th>
    <th>Occasion</th>
    <th>District</th>
  </tr>

<% @tickets.each do |ticket| %>
  <tr>
    <td><%=h ticket.state %></td>
    <td><%=h ticket.group_id %></td>
    <td><%=h ticket.event_id %></td>
    <td><%=h ticket.occasion_id %></td>
    <td><%=h ticket.district_id %></td>
    <td><%= link_to 'Show', ticket %></td>
    <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
    <td><%= link_to 'Destroy', ticket, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ticket', new_ticket_path %>