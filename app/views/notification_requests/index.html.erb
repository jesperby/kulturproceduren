<h1>Listing notification_requests</h1>

<table>
  <tr>
    <th>Send mail</th>
    <th>Send sms</th>
    <th>Group</th>
    <th>Occasion</th>
  </tr>

<% @notification_requests.each do |notification_request| %>
  <tr>
    <td><%=h notification_request.send_mail %></td>
    <td><%=h notification_request.send_sms %></td>
    <td><%=h notification_request.group_id %></td>
    <td><%=h notification_request.occasion_id %></td>
    <td><%= link_to 'Show', notification_request %></td>
    <td><%= link_to 'Edit', edit_notification_request_path(notification_request) %></td>
    <td><%= link_to 'Destroy', notification_request, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New notification_request', new_notification_request_path %>