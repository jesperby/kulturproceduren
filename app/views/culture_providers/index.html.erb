<%= render :partial => "menu" %>

<table>
  <thead>
    <tr>
      <th><%= sort_link "Namn", "name" -%></th>
      <th>Detaljer</th>
    </tr>
  </thead>

  <tbody>

    <% @culture_providers.each do |provider| -%>
      <tr class="<%= cycle('', 'even') %>">
        <td><%= link_to h(provider.name), provider %></td>
        <td><%= link_to "Visa", provider %></td>
      </tr>
    <% end -%>

  </tbody>
</table>

<%= will_paginate @culture_providers %>
